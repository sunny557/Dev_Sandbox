<aura:component >
    <aura:attribute name="num1" type="Integer" default="30"/>
	<aura:attribute name="num2" type="Integer" default="20"/>
    <aura:attribute name="sum" type="Integer" />
    {!v.num1} + {!v.num2} = {!v.sum}
	<br></br>
    <ui:button label="Press Me/Click Me" press="{!c.add}"/>
    	
</aura:component>